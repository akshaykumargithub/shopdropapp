<set xmlns:android="http://schemas.android.com/apk/res/android" android:ordering="sequentially" >
    <objectAnimator
        android:duration="1000"
        android:propertyName="rotationY"
        android:valueFrom="0"
        android:valueTo="180" >
    </objectAnimator>
    <objectAnimator
        android:duration="1000"
        android:propertyName="rotationY"
        android:valueFrom="180"
        android:valueTo="360" >
    </objectAnimator>
  </set>