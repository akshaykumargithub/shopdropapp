<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item

        >
        <shape android:shape="rectangle">
            <solid android:color="@color/main_color_gray_lt" />
        </shape>
    </item>
    <item
        android:bottom="15dp"
        android:left="15dp"
        android:right="15dp"
        android:top="20dp">
        <shape android:shape="rectangle">
            <corners android:radius="5dp" />

            <solid android:color="@color/white_smoke" />
        </shape>
    </item>
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <corners android:radius="0dp" />

            <solid android:color="@color/white" />
        </shape>
    </item>

</layer-list>