<layout xmlns:android="http://schemas.android.com/apk/res/android">
    <data/>

    <LinearLayout
        android:orientation="vertical" android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/MageNative_dynamci_rating_code"
                android:textSize="@dimen/txt_11sp"
                android:textColor="@color/black"
                android:paddingStart="@dimen/pad_10dp"
                />

            <RatingBar
                android:layout_width="192dp"
                android:layout_height="wrap_content"
                android:paddingStart="@dimen/pad_10dp"
                android:numStars="5"
                android:isIndicator="false"
                android:progressTint="@color/ratingbar"
                android:progressBackgroundTint="@color/main_color_gray_dk"
                style="?android:ratingBarStyleIndicator"
                android:id="@+id/MageNative_rating_dynamic"
                android:layout_marginStart="30dp"
                />
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/MageNative_rating_id"
                android:visibility="gone"
                />

        </LinearLayout>
    </LinearLayout>
</layout>